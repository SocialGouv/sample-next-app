{
  "extends": [
    "@socialgouv/eslint-config-recommended",
    "@socialgouv/eslint-config-react"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "."]
      }
    }
  },
  "rules": {
    "jsx-a11y/anchor-is-valid": "off",
    "react/prop-types": "warn",
    "jsx-a11y/label-has-associated-control": "warn"
  }
}
